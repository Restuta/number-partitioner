{
  "name": "number-partitioner",
  "version": "0.1.1",
  "description": "Gernerates partitions for positive interger (https://en.wikipedia.org/wiki/Partition_(number_theory))",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --quiet index.js",
    "tape": "nodemon --quiet test-runner-babel tests/**.js | tap-spec",
    "test": "nodemon --quiet test-runner-babel tests/**.js | faucet"
  },
  "keywords": [
    "number-theory",
    "partitioning",
    "generator",
    "partitions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Restuta/number-partitioner"
  },
  "author": "Anton Vynogradenko <restuta8@gmail.com> (http://restuta.me)",
  "license": "ISC",
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "faucet": "0.0.1",
    "nodemon": "^1.8.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  }
}
